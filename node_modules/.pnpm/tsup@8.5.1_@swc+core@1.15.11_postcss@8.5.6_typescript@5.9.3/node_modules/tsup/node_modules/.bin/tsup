#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_postcss@8.5.6_typescript@5.9.3/node_modules/tsup/dist/node_modules:/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_postcss@8.5.6_typescript@5.9.3/node_modules/tsup/node_modules:/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_postcss@8.5.6_typescript@5.9.3/node_modules:/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_postcss@8.5.6_typescript@5.9.3/node_modules/tsup/dist/node_modules:/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_postcss@8.5.6_typescript@5.9.3/node_modules/tsup/node_modules:/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_postcss@8.5.6_typescript@5.9.3/node_modules:/Users/jacek/src/fsh/nestjs-common/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../dist/cli-default.js" "$@"
fi
